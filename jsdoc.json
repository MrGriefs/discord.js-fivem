{
  "plugins": ["plugins/markdown"],
  "markdown": {
      "parser": "gfm",
      "hardwrap": true
  },
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "readme": "./README.md",
    "verbose": true,
    "destination": "./docs/",
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
        "theme": "dark"
    }
  },
  "source": {
    "include": [ "src" ]
  }
}